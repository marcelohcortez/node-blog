{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js"
      }
    ],
    "functions": {
        "/*": {
          "maxDuration": 45 // This function can run for a maximum of 30 seconds
        }
      }
  }